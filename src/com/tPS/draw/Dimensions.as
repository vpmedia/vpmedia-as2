/** * Dimensions * @author	tPS * @version 1 **/class com.tPS.draw.Dimensions{	private var width:Number;	private var height:Number;	function Dimensions($x:Number,$y:Number){		width = ($x!= undefined) ? $x : 0;		height = ($y!= undefined) ? $y : 0;	}	public function fromArray($Dimensions:Array):Void{		width = $Dimensions[0];		height = $Dimensions[1];	}	public function fromNumbers($x:Number,$y:Number):Void{		width = ($x!= undefined) ? $x : 0;		height = ($y!= undefined) ? $y : 0;	}	public function set _width($w:Number):Void{		width = $w;	}			public function get _width():Number{		return width;	}		public function set _height($h:Number):Void{		height = $h;	}	public function get _height():Number{		return height;	}			public function toString():Void{		trace("Dimensions: _width: " + width + ", _height: " + height);	}}