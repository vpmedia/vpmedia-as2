/** * DataDumper Class * @author	tPS * @version 1 **/class com.tPS.utils.DataDumper{		static public function dump($data:Object,recursion:Boolean):Void{		trace("starting Dump of: " + $data + "\r-----------------------------------");		for(var i in $data){			trace(i + " [" + typeof($data[i]) + "] : " + $data[i]);			if(recursion){				dump($data[i],true);			}		}			}	}